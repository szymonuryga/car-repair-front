{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["warn", {
        "singleQuote": true,
        "endOfLine": "auto",
        "parser": "typescript",
        "printWidth": 150
      }],
      "import/first": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    }
  }